{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Libraries/CMSIS",
                "${workspaceFolder}/Libraries/STM32F4xx_StdPeriph_Driver/inc",
                "${workspaceFolder}/User",
                "${workspaceFolder}/User/FreeRTOS/include",
                "${workspaceFolder}/User/FreeRTOS/portable/RVDS/ARM_CM4F"
            ],
            "defines": [
                "USE_STDPERIPH_DRIVER",
                "STM32F40_41xxx"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}
